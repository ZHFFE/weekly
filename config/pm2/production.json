{
  "apps": [
    {
      "name": "weekly",
      "cwd": "/data/bw/week",
      "script": "server/app.js",
      "exec_mode": "cluster",
      "node_args": "--harmony",
      "env": {
        "NODE_ENV": "production"
      },
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "/dev/null",
      "out_file": "/dev/null",
      "pid_file": "/dev/null",
      "max_memory_restart": "800M",
      "instances": "max"
    }
  ]
}
